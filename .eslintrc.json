{
  "env": {
    "node": true,
    "browser": true,
    "es2021": true
  },
  "extends": ["standard", "next/core-web-vitals", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "plugins": ["eslint-plugin-prettier"],
  "rules": {
  "prettier/prettier": "off",
  "no-empty-pattern": "off",
  "no-unused-vars": "off",
  "react-hooks/exhaustive-deps": "off",
  "react-hooks/rules-of-hooks": "off"
  }
}
